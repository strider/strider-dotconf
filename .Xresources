Xft.dpi:96
Xft.antialias:1
Xft.hinting:1
Xft.hintstyle:hintslight
Xft.rgba:rgba
Xft.lcdfilter:lcddefault

*customization: -color
Xterm*termName: xterm-256color
URxvt*termName: rxvt-256color

!! fonts
*.font: xft:Terminus for Powerline:style=Regular:size=08;hinting=True;antialias=True
*.boldFont: xft:Terminus for Powerline:style=Bold:size=08;hinting=True;antialias=True

URxvt*transparent: true
URxvt*.shading: 25

!! URxvt config
URxvt.depth: 32
!URxvt.background: [85]#1b1d1e
URxvt.perl-ext-common: default,matcher,tabbed,font-size
URxvt.scrollBar: off
URxvt.borderless: true
URxvt.perl-lib: /usr/lib64/urxvt/perl

! url config
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list

! Font-size !
URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal
URxvt.iso14755: false
URxvt.iso14755_52: false

!! manpage colors
URxvt.colorIT:   #87af5f
URxvt.colorBD:   #d7d7d7
URxvt.colorUL:   #87afd7

!Theme Twenty
*foreground: rgb:a/aa/aa
*background: rgb:00/00/00
*color0:     rgb:30/34/30
*color1:     rgb:bf/79/79
*color2:     rgb:97/b2/6b
