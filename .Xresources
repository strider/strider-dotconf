Xft.dpi:96
Xft.antialias:1
Xft.hinting:1
Xft.hintstyle:hintslight
Xft.rgba:rgba
Xft.lcdfilter:lcddefault

XTerm*locale: true
Xterm*borderWidth: 0

*customization: -color

xterm*termName:  xterm-256color
xterm*faceName: xft:Ubuntu\ Mono\ for\ Powerline
xterm*faceSize: 11
xterm*font: xft:Ubuntu\ Mono\ for\ Powerline:pixelsize=11:antialias=true:hinting=true
xterm*imFont: xft:Ubuntu\ Mono\ for\ Powerline:pixelsize=11:antialias=true:hinting=true
xterm*boldFont: xft:Ubuntu\ Mono\ for\ Powerline:bold:pixelsize=11:antialias=true:hinting=true
xterm*italicFont: xft:Ubuntu\ Mono\ for\ Powerline:italic:pixelsize=11:antialias=true:hinting=true
!!xterm*bolditalicFont: xft:Ubuntu\ Mono\ for\ Powerline:bold italic:pixelsize=11:antialias=true:hinting=true

*background: rgb:12/12/12
*foreground: rgb:ff/ff/ff
*color0:     rgb:00/00/00
*color1:     rgb:d3/62/65
*color2:     rgb:ae/ce/91
*color3:     rgb:e7/e1/8c
*color4:     rgb:7a/7a/b0
*color5:     rgb:96/3c/59
*color6:     rgb:41/81/79
*color7:     rgb:be/be/be
*color8:     rgb:66/66/66
*color9:     rgb:ef/81/71
*color10:    rgb:e5/f7/79
*color11:    rgb:ff/f7/96
*color12:    rgb:41/86/be
*color13:    rgb:ef/9e/be
*color14:    rgb:71/be/be
*color15:    rgb:ff/ff/ff

XTerm*pointerColor: white
XTerm*pointerColorBackground: black
XTerm*cursorColor: yellow
XTerm*cursorBlink: true

XTerm*selectToClipboard: true
XTerm*SaveLines: 9000
XTerm*ScrollBar: false
XTerm*visualbell: true
XTerm*bellIsUrgent: true
XTerm*fullscreen: never
XTerm*charClass: 33:48,36-47:48,58-59:48,61:48,63-64:48,95:48,126:48

*VT100*translations: #override Meta <Btn1Up>: exec-formatted("google-chrome-unstable '%t'", PRIMARY)
